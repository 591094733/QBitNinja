	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.All" Version="2.0.0" />
		<PackageReference Include="NBitcoin.Indexer" Version="3.0.0.3" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="App_Start\" />
	</ItemGroup>

